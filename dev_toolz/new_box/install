### apt-get
apt-get install vim vim-common vim-gui-common curl
apt-get install nginx redis-server redis-tools git terminator tmux
apt-get install newsboat pokerth
apt-get install nodejs npm wget rsync gnome-calculator

### npm
npm install express async aws-sdk ejs gm express-session connect-busboy express-session
npm install --save path

### npm sudo other
sudo npm install -g --force nodemon
sudo npm install express-generator -g
sudo npm install -g mocha


